					//				控制菜单点击时的样式
					var Ul = document.getElementById('mylist');
					var Li = document.getElementsByClassName('my');
					for(var i = 0; i < Li.length; i++) {
						Li[i].ontouchstart = function() {
							for(var i = 0; i < Li.length; i++) {
								Li[i].className = 'mui-table-view-cell my';
								Li[i].style.background = '#333';
							}
							this.className = 'mui-table-view-cell my mui-active';
							this.style.background = '#333';
						}
					}
